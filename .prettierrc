{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react",
    "^next",
    "<THIRD_PARTY_MODULES>",
    "^~/public/(.*)$",
    "^~/types/(.*)$",
    "^~/hooks/(.*)$",
    "^~/components/(.*)$",
    "^~/utils/(.*)$",
    "^~/lib/(.*)$",
    "^~/services/(.*)$",
    "^~/constants/(.*)$",
    "^~/api/(.*)$",
    "^[./]"
  ]
}
